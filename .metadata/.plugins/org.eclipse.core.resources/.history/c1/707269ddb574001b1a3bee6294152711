package de.upb.crypto.zeroknowledge.generator

/**
 * A class to collect import statements required for
 * the generated code
 */
class ImportBuilder {
	StringBuilder imports;
	
	new() {
		imports = new StringBuilder();
	}
	
	
	def StringBuilder getBuilder() {
		return imports;
	}
	
	def String getCode() {
		return imports.toString();
	}
}