package de.upb.crypto.zeroknowledge.helpers

class ModelMapControl {
	
	boolean breakTraversal;
	boolean continueIteration;
	
	new() {
		this.breakTraversal = false;
		this.continueIteration = false;
	}
	
	def public void break() {
		this.breakTraversal = true;
	}
	
	def public void continue() {
		this.continueIteration();
	}
	
	
}